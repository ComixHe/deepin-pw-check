#: misc/pam-module/pam_auth.c:283
#, c-format
msgid "Verification error"
msgstr "Doörulama xətası"

#: misc/pam-module/pam_auth.c:287
#, c-format
msgid "Fingerprint verification unavailable, please input password"
msgstr "Barmaq izi doğrulaması mümkün deyil, lütfən şifrəni daxil edin"

#: misc/pam-module/pam_auth.c:307 misc/pam-module/pam_auth.c:315
#, c-format
msgid "Clean your finger and try again"
msgstr "Barmağınızı təmizləyin və yenidən cəhd edin"

#: misc/pam-module/pam_auth.c:311
#, c-format
msgid "Finger not centered"
msgstr "Barmaq sensorun ortasında deyil"

#: misc/pam-module/pam_auth.c:319
#, c-format
msgid "Unclear fingerprint, please clean your finger and try again"
msgstr ""
"Barmaq təmiz deyil, lütfən barmağınızı təmizləyin və yenidən cəhd edin"

#: misc/pam-module/pam_auth.c:323
#, c-format
msgid "Finger moved too fast, please do not lift until prompted"
msgstr ""
"Barmağı çox tez qaldırdınız, lütfən, cihaz kiliddən çıxana qədər barmağı "
"qaldırmayın"

#: misc/pam-module/pam_auth.c:335 misc/pam-module/pam_auth.c:638
#, c-format
msgid "Password"
msgstr "Şifrə"

#: misc/pam-module/pam_auth.c:440
#, c-format
msgid "Verification successful"
msgstr "Doğrulama uğurlu oldu"

#: misc/pam-module/pam_auth.c:457
#, c-format
msgid "Fingerprint locked, use password please"
msgstr "Barmaq izi kilidləndi, şifrədən istifadə edin"

#: misc/pam-module/pam_auth.c:461
#, c-format
msgid "Verification failed, %d chances left"
msgstr "Doğrulama alınmadı, %d cəhd qaldı"

#: misc/pam-module/pam_auth.c:463
#, c-format
msgid "Verification failed, only one chance left"
msgstr "Doğrulama alınmadı, yalnız bir cəhd qaldı"

#: misc/pam-module/pam_auth.c:589
#, c-format
msgid "Please retry after %d minutes"
msgstr ""

#: misc/pam-module/pam_auth.c:591
#, c-format
msgid "Please retry after one minute"
msgstr ""

#: misc/pam-module/pam_auth.c:600
#, c-format
msgid "Chances left: %d"
msgstr "Qalan cəhd: %d"

#: misc/pam-module/pam_auth.c:635
#, c-format
msgid "Verify your fingerprint or password"
msgstr "Barmaq izi və ya şifrənizi doğrulayın"
