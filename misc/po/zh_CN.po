#: lib/deepin_pw_check.c:516
msgid "Invalid error type"
msgstr "无效错误类型"

#: lib/deepin_pw_check.c:527
msgid "Checking successful"
msgstr "校验成功"

#: lib/deepin_pw_check.c:529
msgid "The password cannot be empty"
msgstr "密码不能为空"

#: lib/deepin_pw_check.c:532
#, c-format
msgid "Password must have at least %d characters"
msgstr "密码长度不能少于%d个字符"

#: lib/deepin_pw_check.c:536
#, c-format
msgid "Password must be no more than %d characters"
msgstr "密码长度不能超过%d个字符"

#: lib/deepin_pw_check.c:540
#, c-format
msgid "Password can only contain %s"
msgstr " 密码只能由%s组成"

#: lib/deepin_pw_check.c:544
#, c-format
msgid "Password must not contain more than %d palindrome characters"
msgstr "密码不得含有连续%d个以上的回文字符"

#: lib/deepin_pw_check.c:547 lib/deepin_pw_check.c:549
#: lib/deepin_pw_check.c:551 lib/deepin_pw_check.c:561
msgid "It does not meet password rules"
msgstr "密码不符合安全要求"

#: lib/deepin_pw_check.c:553
msgid "Do not use common words and combinations as password"
msgstr "密码不能是常见单词及组合"

#: lib/deepin_pw_check.c:555
msgid "Parameter options are invalid"
msgstr "无效参数选项"

#: lib/deepin_pw_check.c:557
msgid "Internal error"
msgstr "内部错误"

#: lib/deepin_pw_check.c:559
msgid "Invalid user"
msgstr "无效用户"
