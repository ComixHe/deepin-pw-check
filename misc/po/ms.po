#: lib/deepin_pw_check.c:719
msgid "Checking successful"
msgstr "Pemeriksaan berjaya"

#: lib/deepin_pw_check.c:721
msgid "The password cannot be empty"
msgstr "Kata laluan tidak boleh kosong"

#: lib/deepin_pw_check.c:724
#, c-format
msgid "Password must have at least %d characters"
msgstr "Kata laluan mesti sekurang-kurangnya %d aksara"

#: lib/deepin_pw_check.c:728
#, c-format
msgid "Password must be no more than %d characters"
msgstr "Kata laluan mestilah tidak lebih daripada %d aksara"

#: lib/deepin_pw_check.c:732
msgid ""
"Password can only contain English letters (case-sensitive), numbers or "
"special symbols (~`!@#$%^&*()-_+=|\\{}[]:\"'<>,.?/)"
msgstr ""

#: lib/deepin_pw_check.c:738
#, c-format
msgid "Password must not contain more than %d palindrome characters"
msgstr ""
"Kata laluan mesti tidak mengandungi lebih daripada %d aksara palindrome"

#: lib/deepin_pw_check.c:742 lib/deepin_pw_check.c:744
#: lib/deepin_pw_check.c:746
msgid "Create a strong password please"
msgstr "Sila cipta satu kata laluan yang kuat"

#: lib/deepin_pw_check.c:748
msgid "Do not use common words and combinations as password"
msgstr "Jangan guna perkataan dan gabungan umum sebagai kata laluan"

#: lib/deepin_pw_check.c:750
msgid "Parameter options are invalid"
msgstr "Pilihan parameter tidak sah"

#: lib/deepin_pw_check.c:752
msgid "Internal error"
msgstr "Ralat dalaman"

#: lib/deepin_pw_check.c:754
msgid "Invalid user"
msgstr "Pengguna tidak sah"

#: lib/deepin_pw_check.c:760
#, c-format
msgid ""
"The password must have at least %d characters, and contain at least %d of "
"the four character types: lowercase letters, uppercase letters, numbers, and"
" symbols"
msgstr ""

#: lib/deepin_pw_check.c:764
msgid "It does not meet password rules"
msgstr "Ia tidak menepati peraturan kata laluan"

#: lib/deepin_pw_check.c:766
msgid "Invalid error type"
msgstr "Jenis ralat tidak sah"
